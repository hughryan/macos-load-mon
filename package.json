{
	"name": "macos-load-mon",
	"author": "Hugh McDonald",
	"description": "MacOS Load Monitor",
	"version": "1.0.0",
	"license": "MIT",
	"main": "index.js",
	"scripts": {
		"clean": "rimraf dist",
		"prebuild": "yarn clean",
		"build": "webpack --mode production",
		"prebuildDev": "yarn clean",
		"buildDev": "webpack --mode development",
		"prestart": "yarn build",
		"start": "cross-env NODE_ENV=production node index.js",
		"predev": "yarn buildDev",
		"dev": "cross-env NODE_ENV=development node index.js",
		"lint:style": "stylelint \"**/*.js?(x)\"",
		"lint:code": "eslint \"**/*.js?(x)\"",
		"lint": "yarn lint:code && yarn lint:style",
		"pretest": "yarn lint",
		"test": "nyc mocha",
		"posttest": "rimraf .nyc_output",
		"tdd": "mocha -w --reporter spec",
		"induceLoad": "node ./scripts/induceLoad.js"
	},
	"dependencies": {
		"@nivo/line": "0.50.0",
		"cross-env": "5.2.0",
		"decimal.js": "10.0.1",
		"express": "4.16.4",
		"moment": "2.22.2",
		"prop-types": "15.6.2",
		"react": "16.5.2",
		"react-dom": "16.5.2",
		"recompose": "0.30.0",
		"socket.io": "2.1.1",
		"socket.io-client": "2.1.1",
		"styled-components": "4.0.2",
		"styled-icons": "3.3.0"
	},
	"devDependencies": {
		"@babel/cli": "7.1.2",
		"@babel/core": "7.1.2",
		"@babel/plugin-proposal-object-rest-spread": "7.0.0",
		"@babel/preset-env": "7.1.0",
		"@babel/preset-react": "7.0.0",
		"babel-loader": "8.0.4",
		"babel-plugin-styled-components": "1.8.0",
		"callsite": "1.0.0",
		"chai": "4.2.0",
		"dirty-chai": "2.0.1",
		"eslint": "5.7.0",
		"eslint-config-airbnb-base": "13.1.0",
		"eslint-plugin-import": "2.14.0",
		"eslint-plugin-mocha": "5.2.0",
		"eslint-plugin-react": "7.11.1",
		"file-loader": "2.0.0",
		"html-webpack-plugin": "3.2.0",
		"mocha": "5.2.0",
		"nyc": "13.1.0",
		"proxyquire": "2.1.0",
		"rimraf": "2.6.2",
		"sinon": "7.0.0",
		"sinon-chai": "3.2.0",
		"stylelint": "9.6.0",
		"stylelint-config-recommended": "2.1.0",
		"stylelint-config-styled-components": "0.1.1",
		"stylelint-processor-styled-components": "1.5.0",
		"webpack": "4.21.0",
		"webpack-cli": "3.1.2"
	}
}
